---
layout: null
permalink: /events.ics
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//Bitcoin District//Bitcoin Events in the DMV//EN
{% for event in site.posts %}
BEGIN:VEVENT
UID:{{ event.uid | default: event.date | date: "%Y%m%dT%H%M%S" }}-{{ site.url | replace: "http://", "" | replace: "https://", "" }}
DTSTAMP:{{ site.time | date_to_xmlschema }}
DTSTART:{{ event.date | date_to_xmlschema }}
{% if event.end_date %}DTEND:{{ event.end_date | date_to_xmlschema }}{% endif %}
SUMMARY:{{ event.title }}
{% if event.location %}LOCATION:{{ event.location }}{% endif %}
{% if event.description %}DESCRIPTION:{{ event.description }}{% endif %}
STATUS:{{ event.status | default: "CONFIRMED" }}
END:VEVENT
{% endfor %}
END:VCALENDAR