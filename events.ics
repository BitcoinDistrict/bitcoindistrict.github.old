---
layout: null
permalink: /events.ics
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//Bitcoin District//Bitcoin Events in the DMV//EN
{% for event in site.posts %}
BEGIN:VEVENT
UID:{{ event.uid | default: event.date | date: "%Y%m%dT%H%M%S" }}-{{ event.title | slugify }}-bitcoindistrict.github.io
DTSTAMP:{{ site.time | date: "%Y%m%dT%H%M%SZ" }}
DTSTART:{{ event.date | date: "%Y%m%dT%H%M%SZ" }}
{% if event.end_date %}DTEND:{{ event.end_date | date: "%Y%m%dT%H%M%SZ" }}{% endif %}
SUMMARY:{{ event.title }}
{% if event.location %}LOCATION:{{ event.location }}{% endif %}
{% if event.description %}DESCRIPTION:{{ event.description }}{% endif %}
END:VEVENT
{% endfor %}
END:VCALENDAR